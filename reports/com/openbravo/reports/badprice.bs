report.setSentence("SELECT CSVIMPORT.REFERENCE, CSVIMPORT.CODE,"+
       "CSVIMPORT.NAME, CSVIMPORT.PRICEBUY, CSVIMPORT.PRICESELL"+
        " FROM CSVIMPORT WHERE CSVIMPORT.CSVERROR='New product'");



